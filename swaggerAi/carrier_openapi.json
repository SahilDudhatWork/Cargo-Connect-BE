    "/v1/carrier/vehicle/": {
      "post": {
        "tags": ["Vehicle"],
        "summary": "Create",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "example": {
                  "vehicleName": "truck123",
                  "mxPlates": "MX-ABC-1234",
                  "mxPlatesExpirationDate": "2025-01-01",
                  "usPlates": "US-DEF-5678",
                  "usPlatesExpirationDate": "2025-02-01",
                  "mxInsurancePlates": "MX-INS-9101",
                  "mxInsurancePlatesExpirationDate": "2025-03-01",
                  "usInsurancePlates": "US-INS-1121",
                  "usInsurancePlatesExpirationDate": "2025-04-01",
                  "typeOfService": [
                    {
                      "title": "Express Delivery",
                      "description": "Fast delivery service within 24 hours."
                    },
                    {
                      "title": "Standard Delivery",
                      "description": "Regular delivery service within 3-5 business days."
                    }
                  ],
                  "typeOfTransportation": [
                    {
                      "title": "Refrigerated",
                      "description": "Transport of perishable goods."
                    },
                    {
                      "title": "Dry Goods",
                      "description": "Transport of non-perishable goods."
                    }
                  ],
                  "modeOfTransportation": {
                    "FTL": [
                      {
                        "title": "Full Truckload",
                        "description": "Dedicated truck for one shipment."
                      }
                    ],
                    "LTL": [
                      {
                        "title": "Less Than Truckload",
                        "description": "Shared truck for multiple shipments."
                      }
                    ]
                  },
                  "status": "Active"
                }
              }
            }
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
      }
    },
