          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "companyName": {
                    "type": "string",
                    "example": "Example Logistics Inc."
                  },
                  "contactName": {
                    "type": "string",
                    "example": "John Doe"
                  },
                  "contactNumber": {
                    "type": "integer",
                    "example": "1234567890"
                  },
                  "email": {
                    "type": "string",
                    "example": "johndoe111@example.com"
                  },
                  "password": {
                    "type": "string",
                    "example": "Password123"
                  },
                  "commercialReference[0][companyName]": {
                    "type": "string",
                    "example": "Commercial Partner 1"
                  },
                  "commercialReference[0][contactName]": {
                    "type": "string",
                    "example": "Jane Smith"
                  },
                  "commercialReference[0][emailAddress]": {
                    "type": "string",
                    "example": "jane.smith@partner1.com"
                  },
                  "commercialReference[0][countryCode]": {
                    "type": "integer",
                    "example": "1"
                  },
                  "commercialReference[0][contactNo]": {
                    "type": "integer",
                    "example": "9876543210"
                  },
                  "commercialReference[1][companyName]": {
                    "type": "string",
                    "example": "Commercial Partner 2"
                  },
                  "commercialReference[1][contactName]": {
                    "type": "string",
                    "example": "James Brown"
                  },
                  "commercialReference[1][emailAddress]": {
                    "type": "string",
                    "example": "james.brown@partner2.com"
                  },
                  "commercialReference[1][countryCode]": {
                    "type": "integer",
                    "example": "44"
                  },
                  "commercialReference[1][contactNo]": {
                    "type": "integer",
                    "example": "447123456789"
                  },
                  "companyFormationType": {
                    "type": "string",
                    "example": "usa"
                  },
                  "profilePicture": {
                    "type": "string",
                    "format": "binary"
                  },
                  "scac": {
                    "type": "string",
                    "format": "binary"
                  },
                  "caat": {
                    "type": "string",
                    "format": "binary"
                  },
                  "insurancePolicy": {
                    "type": "string",
                    "format": "binary"
                  },
                  "oea": {
                    "type": "string",
                    "format": "binary"
                  },
                  "ctpat": {
                    "type": "string",
                    "format": "binary"
                  },
                  "companyFormation_usa_w9_Form": {
                    "type": "string",
                    "format": "binary"
                  },
                  "companyFormation_usa_utility_Bill": {
                    "type": "string",
                    "format": "binary"
                  },
                  "companyFormation_maxico_copia_Rfc_Form": {
                    "type": "string",
                    "format": "binary"
                  },
                  "companyFormation_maxico_constance_Of_Fiscal_Situation": {
                    "type": "string",
                    "format": "binary"
                  },
                  "companyFormation_maxico_proof_of_Favorable": {
                    "type": "string",
                    "format": "binary"
                  },
                  "companyFormation_maxico_proof_Of_Address": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {}
            }
          }
        }
